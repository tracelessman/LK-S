{
  "name": "myapp",
  "version": "0.0.5",
  "description": "LK messaging server",
  "author": "spirit",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev:web": "cross-env DEBUG=info NODE_ENV=development nuxt",
    "build": "nuxt build",
    "start": "NODE_ENV=production nuxt start",
    "lint": "eslint --ext .js,.vue .",
    "startWs": "pm2 start api/transfer/Server.js --name=lk --node-args='--inspect'",
    "postinstall": "node bin/postinstall",
    "ip": "npm i --production",
    "dev:other": "isOther=true PORT=4000 nuxt",
    "startWs:other": "isOther=true PORT=4000 pm2 start api/transfer/Server.js --name=lk_other",
    "devAll:other": "npm run startWs:other && npm run dev:other",
    "devAll": "npm run startWs && npm run dev",
    "runAllNuxt": "concurrently 'npm run dev &' 'npm run dev:other &' &",
    "startAll": "npm run devAll && npm run devAll:other",
    "transfer": "clear && git pull && node  api/transfer/Server.js",
    "pmStart": " pm2 start bin/start.js --name=web && pm2 start api/transfer/Server.js --name=transfer",
    "updateVersion": "node bin/updateVersion",
    "restartTransfer": "git pull && pm2 restart transfer",
    "deploy": "node bin/nuxtDeploy",
    "flush": "rm -rf log",
    "dev:transfer": "node --inspect api/transfer/Server.js",
    "dev:all": "concurrently 'npm run dev:transfer' 'npm run dev:web'"
  },
  "dependencies": {
    "@ys/collection": "git+https://github.com/spirit1453/node_collection.git#publish",
    "aes-js": "^3.1.1",
    "apn": "^2.2.0",
    "axios": "^0.16.2",
    "body-parser": "^1.18.3",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.1",
    "express": "^4.15.3",
    "express-session": "^1.15.6",
    "iview": "^2.14.3",
    "js-string-compression": "^1.0.1",
    "mysql2": "^1.5.3",
    "node-rsa": "^1.0.0",
    "nuxt": "^2.2.0",
    "os-utils": "0.0.14",
    "qr-image": "^3.2.0",
    "semver": "^5.5.0",
    "sequelize": "^4.38.0",
    "simple-git": "^1.96.0",
    "source-map-support": "^0.4.15",
    "winston": "^3.1.0",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@ys/eslint-config-strict": "github:spirit1453/codebase#strict_eslint",
    "clipboardy": "^1.2.3",
    "concurrently": "^4.0.1",
    "eslint": "^5.12.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.4.0",
    "node-ssh": "^5.1.2",
    "suppose": "^0.6.2"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "repository": "https://github.com/tracelessman/LK-S.git"
}
